cat > Update/VersionCheck.py <<EOF
import json
import os

def load_manifest(path="manifest.json"):
    if not os.path.exists(path):
        return None
    with open(path, "r") as f:
        return json.load(f)

def check_version():
    manifest = load_manifest()
    if not manifest:
        print("Манифест не найден.")
        return
    print(f"Eugram версия: {manifest.get('version')}")
    for module, components in manifest.get("modules", {}).items():
        print(f"Модуль: {module}")
        for name, version in components.items():
            print(f"  {name}: {version}")

if __name__ == "__main__":
    check_version()
